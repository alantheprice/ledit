{
  "name": "cerebras",
  "endpoint": "https://api.cerebras.ai/v1/chat/completions",
  "auth": {
    "type": "bearer",
    "env_var": "CEREBRAS_API_KEY"
  },
  "headers": {},
  "defaults": {
    "model": "zai-glm-4.7",
    "temperature": 1.0,
    "max_tokens": 4096,
    "top_p": 0.95
  },
  "message_conversion": {
    "include_tool_call_id": true,
    "convert_tool_role_to_user": false,
    "reasoning_content_field": "reasoning"
  },
  "streaming": {
    "format": "sse",
    "chunk_timeout_ms": 300000,
    "done_marker": "[DONE]"
  },
  "models": {
    "default_context_limit": 65536,
    "model_overrides": {
      "zai-glm-4.7": 65536
    },
    "pattern_overrides": [],
    "supports_vision": false,
    "vision_model": "",
    "default_model": "zai-glm-4.7",
    "available_models": []
  },
  "retry": {
    "max_attempts": 3,
    "base_delay_ms": 1000,
    "backoff_multiplier": 2.0,
    "max_delay_ms": 30000,
    "retryable_errors": ["rate_limit_exceeded", "temporary_error", "timeout"]
  },
  "cost": {
    "input_token_cost": 0.0,
    "output_token_cost": 0.0,
    "currency": "USD"
  }
}
