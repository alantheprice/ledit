{
  "name": "minimax",
  "endpoint": "https://api.minimax.io/v1/chat/completions",
  "auth": {
    "type": "bearer",
    "env_var": "MINIMAX_API_KEY"
  },
  "headers": {},
  "defaults": {
    "model": "MiniMax-M2.5",
    "temperature": 1.0,
    "max_tokens": 196608,
    "top_p": 1.0,
    "parameters": {
      "reasoning_split": true
    }
  },
  "message_conversion": {
    "include_tool_call_id": true,
    "convert_tool_role_to_user": false,
    "reasoning_content_field": "reasoning_details",
    "arguments_as_json": false
  },
  "streaming": {
    "format": "sse",
    "chunk_timeout_ms": 300000,
    "done_marker": "[DONE]"
  },
  "models": {
    "default_context_limit": 204800,
    "default_max_completion_tokens": 196608,
    "max_completion_overrides": {
      "MiniMax-M2": 196608,
      "MiniMax-M2.1": 196608,
      "MiniMax-M2.5": 196608,
      "MiniMax-M2-Stable": 196608
    },
    "completion_pattern_overrides": [
      {
        "pattern": "MiniMax-.*",
        "context_limit": 196608
      }
    ],
    "model_overrides": {
      "MiniMax-M2": 204800,
      "MiniMax-M2.1": 204800,
      "MiniMax-M2.5": 204800,
      "MiniMax-M2-Stable": 204800
    },
    "pattern_overrides": [
      {
        "pattern": "MiniMax-.*",
        "context_limit": 204800
      }
    ],
    "supports_vision": true,
    "vision_model": "MiniMax-VL-01",
    "default_model": "MiniMax-M2.5",
    "available_models": [
      "MiniMax-M2",
      "MiniMax-M2.1",
      "MiniMax-M2.5",
      "MiniMax-M2-Stable",
      "MiniMax-VL-01"
    ]
  },
  "retry": {
    "max_attempts": 3,
    "base_delay_ms": 1000,
    "backoff_multiplier": 2.0,
    "max_delay_ms": 30000,
    "retryable_errors": ["rate_limit_exceeded", "temporary_error", "timeout"]
  },
  "cost": {
    "input_token_cost": 0.0,
    "output_token_cost": 0.0,
    "currency": "USD"
  }
}
