{
  "name": "deepseek",
  "endpoint": "https://api.deepseek.com/v1/chat/completions",
  "auth": {
    "type": "bearer",
    "env_var": "DEEPSEEK_API_KEY"
  },
  "headers": {},
  "defaults": {
    "model": "deepseek-chat",
    "temperature": 0.7,
    "max_tokens": 4096,
    "top_p": 1.0
  },
  "message_conversion": {
    "include_tool_call_id": true,
    "convert_tool_role_to_user": false,
    "reasoning_content_field": ""
  },
  "streaming": {
    "format": "sse",
    "chunk_timeout_ms": 320000,
    "done_marker": "[DONE]"
  },
  "models": {
    "default_context_limit": 128000,
    "model_overrides": {
      "deepseek-chat": 128000,
      "deepseek-coder": 128000,
      "deepseek-v3": 128000
    },
    "pattern_overrides": [
      {
        "pattern": "deepseek-.*",
        "context_limit": 128000
      }
    ],
    "supports_vision": false,
    "vision_model": "",
    "default_model": "deepseek-chat",
    "available_models": []
  },
  "retry": {
    "max_attempts": 3,
    "base_delay_ms": 1000,
    "backoff_multiplier": 2.0,
    "max_delay_ms": 30000,
    "retryable_errors": ["rate_limit_exceeded", "temporary_error", "timeout"]
  },
  "cost": {
    "input_token_cost": 0.14,
    "output_token_cost": 0.28,
    "currency": "USD"
  }
}