{
  "name": "zai",
  "endpoint": "https://api.z.ai/api/coding/paas/v4/chat/completions",
  "auth": {
    "type": "bearer",
    "env_var": "ZAI_API_KEY"
  },
  "headers": {},
  "defaults": {
    "model": "glm-4.6",
    "temperature": 0.1,
    "top_p": 1.0
  },
  "message_conversion": {
    "include_tool_call_id": false,
    "convert_tool_role_to_user": false,
    "reasoning_content_field": ""
  },
  "streaming": {
    "format": "sse",
    "chunk_timeout_ms": 320000,
    "done_marker": "[DONE]"
  },
  "models": {
    "default_context_limit": 128000,
    "model_overrides": {
      "glm-4.6": 200000,
      "glm-4.5": 128000,
      "glm-4": 128000,
      "glm-3-turbo": 128000
    },
    "pattern_overrides": [
      {
        "pattern": "glm-4\\.6.*",
        "context_limit": 200000
      },
      {
        "pattern": "glm-4\\.[7-9].*",
        "context_limit": 200000
      },
      {
        "pattern": "glm-4.*",
        "context_limit": 128000
      },
      {
        "pattern": "glm-3.*",
        "context_limit": 128000
      }
    ],
    "model_info": [
      {
        "id": "glm-4.7",
        "name": "GLM-4.7",
        "description": "Z.AI GLM-4.7 - Latest GLM model with 200K context window",
        "context_length": 200000,
        "tags": ["tools", "coding"]
      },
      {
        "id": "GLM-4.6",
        "name": "GLM-4.6",
        "description": "Z.AI GLM-4.6 - Coding optimized model with 200K context window",
        "context_length": 200000,
        "tags": ["tools", "coding"]
      },
      {
        "id": "glm-4.6v",
        "name": "GLM-4.6 Vision",
        "description": "Z.AI GLM-4.6 with vision capabilities",
        "context_length": 200000,
        "tags": ["tools", "coding", "vision"]
      },
      {
        "id": "GLM-4.5",
        "name": "GLM-4.5",
        "description": "Z.AI GLM-4.5 - Previous generation with 128K context window",
        "context_length": 128000,
        "tags": ["tools"]
      },
      {
        "id": "GLM-4.5-air",
        "name": "GLM-4.5 Air",
        "description": "Z.AI GLM-4.5 Air - Lightweight version with 128K context window",
        "context_length": 128000,
        "tags": ["tools"]
      }
    ],
    "supports_vision": true,
    "vision_model": "glm-4.6v",
    "default_model": "glm-4.6",
    "available_models": []
  },
  "retry": {
    "max_attempts": 3,
    "base_delay_ms": 1000,
    "backoff_multiplier": 2.0,
    "max_delay_ms": 30000,
    "retryable_errors": ["rate_limit_exceeded", "temporary_error", "timeout"]
  },
  "cost": {
    "input_token_cost": 0.0,
    "output_token_cost": 0.0,
    "currency": "USD"
  }
}